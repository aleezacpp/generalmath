#include <stdio.h>
int func(int t){
    if ((t==1)||(t==2)){
        return 1; //1st and 2nd terms of fabionacci are 1
    }
    else {
        return 
            func(t-1)+func(t-2);
        
    }
}
int main(){
    int t; //term number
    int a; //answer
    printf("t : \n");
    scanf("%d",&t);
    a=func(t);
    printf("%dth term of fabionacci sq : %d \n",t,a);
}
